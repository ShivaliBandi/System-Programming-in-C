build:
	gcc program.c -o program -lpthread

clean:
	rm program

help:
	@echo 'Use as make build'
	
run:
	./program
																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																														
